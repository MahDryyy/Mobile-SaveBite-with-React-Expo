{
  "expo": {
    "name": "SaveBite",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SaveBite.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/SaveBite.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#20ab3c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SaveBite.jpg",
        "backgroundColor": "#20ab3c"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jamalud.myapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e544dab9-5b67-464e-afd0-48a2d91cc112"
      }
    }
  }
}
